    },
    "homepage": "https://github.com/wuhuanhost/node-backend-services#readme",
    "devDependencies": {
        "egg-bin": "^4.7.1",
        "egg-mock": "^3.22.1",
        "eslint": "^5.16.0",
        "eslint-config-standard": "^12.0.0",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-node": "^8.0.1",
        "eslint-plugin-promise": "^4.1.1",
        "eslint-plugin-standard": "^4.0.0",
        "sequelize-cli": "^5.4.0"
